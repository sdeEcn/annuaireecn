{% extends 'default/user.html.twig' %}


{% block scripts %}


            function submit(id){
                var url="{{ path('app.clubAjax.submit',{'id':"id"}) }}";
                url= url.replace('id',id);
                $.ajax(url,
                    {
                        success : function (data) {
                            data = $.parseJSON(data);

                            var name=".club"+id;
                            $(".test").html(name);
                            $(name).remove();

                        },
                        error : function (data) {

                        }
                    });
            };

            function setListen(){
                $(".cancelinsc").on("click",function (event) {
                    var id = $(this).data('club');
                    submit(id);
                });
            }


{% endblock %}
